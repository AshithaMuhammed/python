# validate date

from re import fullmatch
validate_date=input("Enter Date ")
pattern="(0[1-9]|1[0-9]|2[0-9]|3[0-1])"
matcher=fullmatch(pattern,validate_date)
print("invalid" if matcher==None else "valid")