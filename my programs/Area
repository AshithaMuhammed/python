Num_1=input("Enter number 1 : ")
Num_2=input("Enter number 2 : ")
Num_3=input("Enter number 3 : ")

if(Num_1>Num_2):
    if(Num_1>Num_3):
        print(f"The largest number is {Num_1}") 
    else:
        print(f"The largest number is {Num_3}") 
elif(Num_2>Num_3):
    print(f"The largest number is {Num_2}")
else:
    print(f"The largest number is {Num_3}")

